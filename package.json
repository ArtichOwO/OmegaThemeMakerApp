{
  "name": "omega-thmkr",
  "productName": "Omega Theme Maker",
  "version": "1.0.0",
  "description": "A theme maker",
  "main": "main.js",
  "icon": "img/icon.png",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",

    "pack:osx": "electron-packager . $npm_package_productName --out=dist/osx --platform=darwin --arch=x64 --icon=img/icon.icns",
    "pack:win64": "electron-packager . $npm_package_productName --out=dist/win --platform=win32 --arch=x64 --icon=img/icon.ico",
    "build": "npm run pack:osx && npm run pack:win64"
  },
  "repository": "https://github.com/M4xi1m3/Omega-ThMkr",
  "keywords": [
    "Omega",
    "Theme"
  ],
  "author": "M4xi1m3, ArtichautCosmique",
  "license": "¯\\_(ツ)_/¯",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "electron": "^12.0.0"
  },
  "dependencies": {
    "discord-rpc": "^3.2.0",
    "electron-squirrel-startup": "^1.0.0"
  },
  "config": {}
}
